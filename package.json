{
  "name": "CompanyB-BashCoffee",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "dotenv -- turbo run dev --parallel",
    "dev:happybaby": "dotenv -- turbo run dev --filter=\"./apps/happy-baby/*\"",
    "dev:happybaby:liff": "dotenv -- turbo run dev --filter=\"./apps/happy-baby/line-liff-new\"",
    "build": "dotenv -- turbo run build",
    "build:package": "dotenv -- turbo run build --filter=\"./packages/*\"",
    "start": "dotenv -- turbo run start",
    "start:client": "dotenv -- turbo run start --filter=\"./apps/client/*\"",
    "start:server": "dotenv -- turbo run start --filter=\"./apps/server\"",
    "start:happybaby": "dotenv -- turbo run start --filter=\"./apps/happy-baby/*\"",
    "db:push": "dotenv -- pnpm --filter @bash-coffee/db push",
    "db:pull": "dotenv -- pnpm --filter @bash-coffee/db pull",
    "db:migrate:dev": "dotenv -- pnpm --filter @bash-coffee/db migrate:dev",
    "db:migrate:deploy": "dotenv -- pnpm --filter @bash-coffee/db migrate:deploy",
    "db:migrate:reset": "dotenv -- pnpm --filter @bash-coffee/db migrate:reset",
    "db:seed": "dotenv -- pnpm --filter @bash-coffee/db seed",
    "db:studio": "dotenv -- pnpm --filter @bash-coffee/db studio",
    "db:generate": "dotenv -- pnpm --filter @bash-coffee/db generate",
    "db:line:push": "dotenv -- pnpm --filter @bash-coffee/line-db push",
    "db:line:pull": "dotenv -- pnpm --filter @bash-coffee/line-db pull",
    "db:line:migrate:dev": "dotenv -- pnpm --filter @bash-coffee/line-db migrate:dev",
    "db:line:migrate:deploy": "dotenv -- pnpm --filter @bash-coffee/line-db migrate:deploy",
    "db:line:migrate:reset": "dotenv -- pnpm --filter @bash-coffee/line-db migrate:reset",
    "db:line:seed": "dotenv -- pnpm --filter @bash-coffee/line-db seed",
    "db:line:studio": "dotenv -- pnpm --filter @bash-coffee/line-db studio",
    "db:line:generate": "dotenv -- pnpm --filter @bash-coffee/line-db generate",
    "lint": "eslint . --no-error-on-unmatched-pattern",
    "fmt": "prettier --write ."
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "dotenv-cli": "7.4.2"
  },
  "devDependencies": {
    "@types/node": "22.7.9",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "turbo": "1.13.3",
    "typescript": "5.6.3"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "semi": false,
    "arrowParens": "avoid"
  },
  "packageManager": "pnpm@9.7.1"
}